market_opportunity = sales_customers.groupby([""City""]).agg({""CustomerID_x"":""sum"", ""TotalPrice"": ""sum""}).reset_index()
market_opportunity = market_opportunity.rename(columns={""CustomerID_x"":""CustomerCount"", ""TotalPrice"" : ""TotalSales""})
